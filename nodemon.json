{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/**/node_modules",
        "src/__generated__"
    ],
    "verbose": true,
    "execMap": {
        "ts": "ts-node"
    },
    "watch": ["src/"],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js,json,ts"
}